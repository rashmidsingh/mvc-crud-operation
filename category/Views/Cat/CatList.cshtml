@model List<category.Context.Category1>
@{
    ViewBag.Title = "CatList";
}

<table class="table table-hover">
<tr>
    <th>CategoryName</th>

</tr>
<tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.CategoryName</td>

        <td>
            <a href="@Url.Action("Cat",new { item.CategoryId,item.CategoryName})" class="btn btn-success ">Edit</a>
            <a href="@Url.Action("Delete",new { item.CategoryId})" class="btn btn-danger">Delete</a>
        </td>
    </tr>
}

    </table>



